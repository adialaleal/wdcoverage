<%= form_with(model: toner, local: true) do |form| %>
<% if toner.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(toner.errors.count, "error") %> prohibited this toner from being saved:</h2>
  <ul>
    <% toner.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>
<div class="container">
  <form>
    <div class="form-group">
      <label>Cliente</label>
      <%= form.text_field :client %>
      <label>Dispositivo</label>
      <%= form.text_field :device %>
      <label>Nº Serial</label>
      <%= form.text_field :serial_number %>
    </div>
    <div class="form-group">
      <label>Nº Suprimento</label>
      <%= form.text_field :part_number %>
      <label>Cor</label>
      <%= form.text_field :tcolor %>
      <label>Rendimento</label>
      <%= form.number_field :tyield %>
    </div>
    <div class="form-group">
      <label>Contador Inicial</label>
      <%= form.number_field :count_ini %>
      <label>Contador Final</label>
      <%= form.number_field :count_end %>
      <label>Nível Inicial <small>use valores decimais - 0.00</small></label>
      <%= form.text_field :nvl_ini %>
    </div>
    <div class="form-group">
      <label>Nível Final <small>use valores decimais - 0.00</small></label>
      <%= form.text_field :nvl_end %>
    </div>
    <div class="form-group">
      <label>Data Inicial</label>
      <%= form.date_select :date_in %>
      <label>Data Final</label>
      <%= form.date_select :date_out %>
    </div>
    <div class="actions">
      <%= form.submit %>
    </div>
  </form>
</div>
<% end %>